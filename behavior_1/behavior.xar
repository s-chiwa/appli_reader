		  GeneratedClass.__init__( self )

	def onInput_onStart(self, p):
		p = self.typeConversion(p)
		if(p == self.typeConversion("全部")):
			self.output_1(p)
		elif(p == self.typeConversion("エンターテイメント")):
			self.output_2(p)
		elif(p == self.typeConversion("教育")):
			self.output_3(p)
		elif(p == self.typeConversion("ライフスタイル")):
			self.output_4(p)
		elif(p == self.typeConversion("写真や音楽と動画")):
			self.output_5(p)
		elif(p == self.typeConversion("ニュースと情報")):
			self.output_6(p)
		elif(p == self.typeConversion("ゲーム")):
			self.output_7(p)
		elif(p == self.typeConversion("ヘルスケア")):
			self.output_8(p)
		elif(p == self.typeConversion("コミュニケーション")):
			self.output_9(p)
		elif(p == self.typeConversion("子供向け")):
			self.output_10(p)
		elif(p == self.typeConversion("appClose")):
			self.output_11(p)
		elif(p == self.typeConversion("emotionViewOn")):
			self.output_12(p)
		elif(p == self.typeConversion("emotionViewOff")):
			self.output_13(p)
		elif(p == self.typeConversion("ツール")):
			self.output_14(p)
		else:
			self.onDefault()

	def typeConversion(self, p):
		try:
			p = float(p)
			pint = int(p)
			if( p == pint ):
				p = pint
